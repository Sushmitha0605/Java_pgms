import java.util.*;
public class Array{
    public static void main(String[] args){
        int[] array = new int[10]; 
        Random rand = new Random(); 

        for (int i = 0; i < array.length; i++) {
            array[i] = rand.nextInt(10) + 1;
        }

        System.out.print("Array of random integers: ");
        for (int num : array) {
            System.out.print(num + " ");
        }
    }
}
